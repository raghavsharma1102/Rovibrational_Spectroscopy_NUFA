Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"ModifiedMorse", "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["D", "e"], 
      SuperscriptBox["z", "2"]}], "-", 
     RowBox[{"2", 
      SubscriptBox["D", "e"], "z"}]}]}], ";"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
    "Normal", " ", "is", " ", "used", " ", "to", " ", "remove", " ", "the", 
     " ", "big"}], "-", 
    RowBox[{"O", " ", "term"}]}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"pekerisApproximation", "=", 
    RowBox[{"Normal", "[", 
     RowBox[{"Series", "[", 
      RowBox[{
       FractionBox["1", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"1", "-", 
           FractionBox[
            RowBox[{"Log", "[", "z", "]"}], "\[Alpha]"]}], ")"}], "2"]], ",", 
       RowBox[{"{", 
        RowBox[{"z", ",", "1", ",", "2"}], "}"}]}], "]"}], "]"}]}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "Rearrange", " ", "the", " ", "expansion", " ", "by", " ", "powers", " ", 
    "of", " ", 
    RowBox[{"z", ".", " ", "The"}], " ", "following", " ", "is", " ", "an", 
    " ", "approximation", " ", "for", " ", Cell[TextData[Cell[BoxData[
     FormBox[
      FractionBox[
       SuperscriptBox[
        SubscriptBox["r", "e"], "2"], 
       SuperscriptBox["r", "2"]], TraditionalForm]],ExpressionUUID->
     "34fb4f56-9c2f-4170-945e-2d2cf1c36907"]],ExpressionUUID->
     "ba6a95b5-545c-42d3-985a-0b19a8d5d759"]}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"pekerisApproximation", "=", 
  RowBox[{"Collect", "[", 
   RowBox[{
    RowBox[{"Expand", "[", "pekerisApproximation", "]"}], ",", "z"}], 
   "]"}]}]}], "Input",
